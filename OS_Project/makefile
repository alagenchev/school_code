CC = gcc
RELEASE_FLAGS = -O2 -w -std=gnu99
DEBUG_FLAGS = -g -O0 -std=gnu99
SOURCES = *.c
HEADERS = *.h 
EXECUTABLE= a.out
OPTIMIZATION_FLAG = -O
INCLUDES = -I ./
all: 
		$(CC) $(RELEASE_FLAGS) $(SOURCES) $(HEADERS) $(OPTIMIZATION_FLAG) $(INCLUDES) -lrt -o $(EXECUTABLE)
debug:
		
		$(CC) $(DEBUG_FLAGS) $(SOURCES) $(HEADERS) $(INCLUDES) -o $(EXECUTABLE)
clean:
		rm -rf $(EXECUTABLE)
test:
		./$(EXECUTABLE)
